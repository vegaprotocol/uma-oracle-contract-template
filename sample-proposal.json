{
	"dataSourceSpecBinding": {
		"settlementDataProperty": "price",
		"tradingTerminationProperty": "terminated"
	},
	"dataSourceSpecForTradingTermination": {
		"external": {
			"ethOracle": {
				"sourceChainId": "5",
				"address": "0xee05CD6Ed5959c688C857033b82184bC1755ae5c",
				"abi": "[{\"type\":\"function\",\"name\":\"getTermination\",\"inputs\":[{\"name\":\"marketId\",\"type\":\"string\",\"internalType\":\"string\"}],\"outputs\":[{\"name\":\"\",\"type\":\"bool\",\"internalType\":\"bool\"}],\"stateMutability\":\"view\"}]",
				"method": "getTermination",
				"args": [
					"MARKET_NAME_REPLACE_ME_HERE"
				],
				"requiredConfirmations": "3",
				"trigger": {
					"timeTrigger": {
						"every": "60"
					}
				},
				"filters": [
					{
						"key": {
							"name": "terminated",
							"type": "TYPE_BOOLEAN"
						},
						"conditions": [
							{
								"operator": "OPERATOR_EQUALS",
								"value": "true"
							}
						]
					}
				],
				"normalisers": [
					{
						"name": "terminated",
						"expression": "$[0]"
					}
				]
			}
		}
	},
	"dataSourceSpecForSettlementData": {
		"external": {
			"ethOracle": {
				"sourceChainId": "5",
				"address": "0x19BCDbF79a20B195Da12Fe6Cd35C928793fE2AE7",
				"abi": "[{\"type\":\"function\",\"name\":\"getSettlementPrice\",\"inputs\":[{\"name\":\"marketId\",\"type\":\"string\",\"internalType\":\"string\"}],\"outputs\":[{\"name\":\"\",\"type\":\"uint256\",\"internalType\":\"uint256\"}],\"stateMutability\":\"view\"}]",
				"method": "getSettlementPrice",
				"args": [
					"MARKET_NAME_REPLACE_ME_HERE"
				],
				"requiredConfirmations": "3",
				"trigger": {
					"timeTrigger": {
						"every": "60"
					}
				},
				"filters": [
					{
						"key": {
							"name": "price",
							"type": "TYPE_INTEGER",
							"numberDecimalPlaces": "18"
						},
						"conditions": [
							{
								"operator": "OPERATOR_GREATER_THAN",
								"value": "0"
							}
						]
					}
				],
				"normalisers": [
					{
						"name": "price",
						"expression": "$[0]"
					}
				]
			}
		}
	}
}
